{
  "name": "coder-ipsum-website",
  "version": "1.0.0",
  "description": "Website for generating coder-inspired ipsum text using the coder-ipsum npm package",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "webpack -p && touch dist/.nojekyll && cp CNAME dist/CNAME",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "author": "Sia Karamalegos",
  "license": "MIT",
  "dependencies": {
    "coder-ipsum": "^1.1.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
